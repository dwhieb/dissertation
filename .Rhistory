library(ggplot2)
load_data <- function(file_path) {
data <- read.table(
file_path,
colClasses = c(
"character",
"integer",
"integer",
"integer",
"integer",
"numeric"
),
col.names = c(
"stem",
"REF",
"PRED",
"MOD",
"tokens_observed",
"flexibility"
),
comment.char = "",
encoding     = "UTF-8",
header       = FALSE,
quote        = "",
sep          = "\t",
)
return(data)
}
